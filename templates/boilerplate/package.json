  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx concurrently -k -s first \"lamdera live --port=${PORT:-8000}\" \"npx tailwindcss -i ./src/styles.css -o ./public/styles.css --watch\"",
